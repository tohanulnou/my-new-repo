//For storing the search history of the data to be searched in if direct
function storeSearchHistory(value,textbox) {
  var storedSearches = [];
  var searchHistory="";

  if(textbox=="AEFGlobalFullTextSearch"){
  	searchHistory=localStorage.GlobalHistory;
  }
  else if(textbox=="NAME"){
  	searchHistory=localStorage.NameHistory;
  }
  else if(textbox=="TITLE"){
  	searchHistory=localStorage.TitleHistory;
  }

  if (!searchHistory) {
      storedSearches.push(value);
      searchHistory = JSON.stringify(storedSearches);
  } else {
      //get the stored array
      storedSearches = JSON.parse(searchHistory);
      if (storedSearches.indexOf(value) === -1) {
          //if value not present
          //keep last 20 (distinct) searches
          if (storedSearches.length >= 20) storedSearches.shift();
          storedSearches.push(value);
          searchHistory = JSON.stringify(storedSearches);
      } else {
          //if value present, move it at the end
          var position = storedSearches.indexOf(value);
          var atPosition = storedSearches[position];
          storedSearches.splice(position, 1);
          storedSearches.push(atPosition);
          searchHistory = JSON.stringify(storedSearches);
      }
  }
  if(textbox=="NAME"){
  	localStorage.NameHistory=searchHistory;
  }
  else if(textbox=="TITLE"){
  	localStorage.TitleHistory=searchHistory;
  }
  else if(textbox=="AEFGlobalFullTextSearch"){
  	localStorage.GlobalHistory=searchHistory;
  }
}

