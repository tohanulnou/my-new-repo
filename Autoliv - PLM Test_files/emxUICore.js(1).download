var strUserAgent = navigator.userAgent.toLowerCase();
var isIE =( strUserAgent.indexOf("msie") > -1  ||  !!strUserAgent.match(/trident.*rv[ :]*11\./))&& strUserAgent.indexOf("opera") == -1;
var isIE11 = !!strUserAgent.match(/trident.*rv[ :]*11\./)
var isMinIE5 = false,isMinIE55 = false,isMinIE6 = false,isMinIE9 = false,isMaxIE8 = false;
if (isIE || isIE11) {
        var reIE = new RegExp("msie (\\S*);");
        reIE.test(strUserAgent);
        var fVer = parseFloat(RegExp["$1"]);
        isMinIE5 = fVer >= 5;
        isMinIE55 = fVer >= 5.5;
        isMinIE6 = fVer >= 6;
        isMinIE9 = fVer >= 9;
        isMaxIE8 = fVer <= 8;
}
var isGecko = strUserAgent.indexOf("gecko") > -1;
var isMoz = isGecko && !isIE11;
var isMinMoz092 = false,isMinMoz094 = false,isMinMoz098 = false,isMinMoz1 = false,isMinFF3 = false,isMinFF4 = false,isMinMoz19 = false;
if (isMoz) {
        if (strUserAgent.indexOf("rv:") > -1) {
                var reMoz = new RegExp("rv:([\\d\\]*.\\d)([\\.\\d]*)");
                reMoz.test(strUserAgent);
                var fMajorVer = RegExp["$1"];
                var fMinorVer = RegExp["$2"];
                isMinMoz092 = (fMajorVer == 0.9 && fMinorVer >= 0.2) || (fMajorVer > 0.9);
                isMinMoz094 = (fMajorVer == 0.9 && fMinorVer >= 0.4) || (fMajorVer > 0.9);
                isMinMoz098 = (fMajorVer == 0.9 && fMinorVer >= 0.8) || (fMajorVer > 0.9);
                isMinMoz1 = (fMajorVer >= 1.0);
                // check for firefox 3
                isMinMoz19 = (fMajorVer >= 1.9);
        }
        var reFF = new RegExp("firefox[\/\s]([\\d\\]*.\\d)([\\.\\d]*)");
        reFF.test(strUserAgent);
        var ffVer = parseFloat(RegExp["$1"]);
        isMinFF3 = ffVer >= 3;
        isMinFF4 = ffVer >= 4;
}
