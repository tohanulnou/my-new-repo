function getTypeAheadItem(texboxBox){
	if(emxUIConstants.STR_FIELD_DIRECT_ENTRY){
	    jQuery('#'+texboxBox.name).autocomplete({
    		source: function(request, response){
    			var rows = imAutocompleteJSONParse(texboxBox);
    			return response(rows);
    		},
    		select: function(event, ui){
    			var hasValue = (ui.item.value != undefined && ui.item.value != "" && ui.item.value != null);
    			if (hasValue) {
    				var focusedElement = $(this);
    				focusedElement.val(ui.item.name);
    				var selectItem="";
    				selectItem=jQuery('#hidden_'+texboxBox.name).val();
					var filter =ui.item.pre+ui.item.value;
					ui.item.value=ui.item.pre+ui.item.value+"|";
					if(ui.item.pre==""){
						FullSearch.addToFilters(texboxBox.name, filter);
					}else{
						FullSearch.appendToFilters(texboxBox.name, filter);
					}
    				jQuery('#hidden_'+texboxBox.name).val(ui.item.value);

    			}
    		},
    		focus: function( event, ui ) {
    			var hasValue = (ui.item.value != undefined && ui.item.value != "" && ui.item.value != null);
    			if ((hasValue && ui.item.pre!=undefined)||ui.item.message) {
    				return false;
    			}
    		},
    		minLength:1,
    		delay:1000
    	});
	}
}
function imAutocompleteJSONParse(texboxBox){
	var typedValue = texboxBox.value;
	var preValue="";
	if(typedValue.indexOf("|")>0){
		var preValue = typedValue.substring(0,typedValue.lastIndexOf("|")+1);
		var typedValues=typedValue.split("|")
		var typeLength = typedValues.length;
		typedValue = typedValues[typeLength-1].trim();
	}
	var charCount = typedValue.length;
	if(charCount<2){
		return;
	}
	var typeChar=typedValue;
	var rows =[];
	var data="typeChar="+typeChar+"&fieldType="+texboxBox.name;
	jQuery.ajax({
        type: "GET",
        async: false,
        url: "../common/emxFullSearchTypeAhead.jsp",
		data: data,
        success: function(data) {
        	var rowData = null;
        	var dataLength = data.length;
			if(!data[0].message){
        	for(var i=0;i<dataLength;i++){
        		rowData = data[i];
        		rows[i] = {
        			id: rowData.id,
						name:rowData.name,
        			pre: preValue,
        			label:rowData.fullname+'('+rowData.name+')',
						value:rowData.name
					};
				}
			}else{
				rows[0] = {
						label:data[0].message,
						value:data[0].message,
						message:data[0].message
        		};
        	}
         }
	});
	return rows;
}
